apt-get -qq update
apt-get -y install wget
apt-get -y install unzip
wget https://ci.inria.fr/moose/job/moose-6.1/lastSuccessfulBuild/artifact/moose-6.1.zip
unzip moose-6.1.zip
wget http://files.pharo.org/get-files/60/sources.zip
unzip sources.zip
http://files.pharo.org/get-files/50/pharo-linux-stable.zip
unzip pharo-linux-stable.zip
./pharo --headless moose-6.1.image eval --save "Gofer it repository: (MCFileTreeRepository new directory: (FileLocator imageDirectory / 'parsec')); package: 'TotParser'; load."
./pharo --headless moose-6.1.image test "TotParser"
